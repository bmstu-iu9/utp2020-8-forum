<ul class="ulCategories noUlDecor noUlDefaultPadding">
    <h3 style="background: white; margin-bottom: 0" class="category colorTextDark category-link">
        Все
    </h3>
    {{#each categories}}
        <li class="category colorSectionCategory colorDarkLink">
            <a class="category-link colorTextDark"
               href="/category/{{this.id}}">{{this.name}}</a>
        </li>
    {{/each}}
</ul>